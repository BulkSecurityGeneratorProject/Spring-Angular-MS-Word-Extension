<?xml version="1.0" encoding="UTF-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns='http://www.ehcache.org/v3'
         xsi:schemaLocation="http://www.ehcache.org/v3 http://www.ehcache.org/schema/ehcache-core-3.0.xsd">

    <cache-template name="simple">
        <expiry>
            <ttl unit="seconds">3600</ttl>
        </expiry>
        <heap>100</heap>
    </cache-template>

    <cache alias="be.storefront.imicloud.domain.User" uses-template="simple"/>

    <cache alias="be.storefront.imicloud.domain.Authority" uses-template="simple"/>

    <cache alias="be.storefront.imicloud.domain.User.authorities" uses-template="simple"/>

    <cache alias="be.storefront.imicloud.domain.PersistentToken" uses-template="simple"/>

    <cache alias="be.storefront.imicloud.domain.User.persistentTokens" uses-template="simple"/>

    <cache alias="be.storefront.imicloud.domain.Organization" uses-template="simple"/>

    <cache alias="be.storefront.imicloud.domain.Organization.folders" uses-template="simple"/>

    <cache alias="be.storefront.imicloud.domain.Folder" uses-template="simple"/>

    <cache alias="be.storefront.imicloud.domain.Folder.documents" uses-template="simple"/>

    <cache alias="be.storefront.imicloud.domain.ImDocument" uses-template="simple"/>

    <cache alias="be.storefront.imicloud.domain.ImDocument.maps" uses-template="simple"/>

    <cache alias="be.storefront.imicloud.domain.ImMap" uses-template="simple"/>

    <cache alias="be.storefront.imicloud.domain.ImMap.blocks" uses-template="simple"/>

    <cache alias="be.storefront.imicloud.domain.ImBlock" uses-template="simple"/>

    <cache alias="be.storefront.imicloud.domain.ImBlock.images" uses-template="simple"/>

    <cache alias="be.storefront.imicloud.domain.Image" uses-template="simple"/>

    <cache alias="be.storefront.imicloud.domain.Image.imBlocks" uses-template="simple"/>

    <cache alias="be.storefront.imicloud.domain.Branding" uses-template="simple"/>

    <cache alias="be.storefront.imicloud.domain.ImageSourcePath" uses-template="simple"/>

    <!-- jhipster-needle-ehcache-add-entry -->
</config>
